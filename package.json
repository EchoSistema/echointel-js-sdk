{
  "name": "@echointel/echointel-sdk",
  "version": "1.0.0",
  "description": "EchoIntel AI SDK for JavaScript/TypeScript",
  "author": "EchoSistema S.A.",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "echointel",
    "ai",
    "analytics",
    "forecasting",
    "machine-learning",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EchoSistema/echointel-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/EchoSistema/echointel-js-sdk/issues"
  },
  "homepage": "https://github.com/EchoSistema/echointel-js-sdk#readme"
}
